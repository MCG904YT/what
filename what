local ta=game.CoreGui.SBM.main.Information.ta
ta:connect(function()
	while true do
		wait(0.1)
		local num = 0
		for i,v in pairs(game:GetService("Players").LocalPlayer.Backpack:GetChildren()) do
			num = num + 1
		end
		ta.Text = num .. " Tools"
		if num >= 9999999999999 then
			break
		end
	end
end)
